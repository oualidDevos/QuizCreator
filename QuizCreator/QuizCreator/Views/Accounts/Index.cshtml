@model IEnumerable<QuizCreator.Models.Account>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="container">
    <div class="row">
        <div class="col-sm-3">
            <p>
                @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-danger" })
            </p>
        </div>
        <div class="col-sm-3">
            <p>
                @Html.ActionLink("Create Questions", "Create", "Question", null, new { @class = "btn btn-success" })
            </p>
        </div>
        <div class="col-sm-3">
            <p>
                @Html.ActionLink("Users List", "Users", null, new { @class = "btn btn-primary" })
            </p>
        </div>
        <div class="col-sm-3">
            <p>
                @Html.ActionLink("Dashboard", "Dashboard", null, new { @class = "btn btn-info" })
            </p>
        </div>
    </div>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdAccount }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdAccount }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdAccount })
            </td>*@
        </tr>
    }

</table>
