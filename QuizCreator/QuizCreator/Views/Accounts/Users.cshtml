@model List<QuizCreator.Models.Account>

@{
    ViewBag.Title = "Users";
}

<ul>
    <li>Accounts : @Model.Count</li>
</ul>
<table class="table">
    <tr>
        <th>
            Email
        </th>
        <th>
            UserName
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdAccount }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdAccount }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdAccount })
        </td>
    </tr>
}

</table>
